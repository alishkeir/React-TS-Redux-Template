{
    "name": "react-redux-ts-template",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/node": "^17.0.45",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/react-router-dom": "^5.3.3",
        "env-cmd": "^10.1.0",
        "prettier": "^2.8.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.8.0",
        "react-scripts": "5.0.1",
        "sass": "^1.58.0",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "eject": "react-scripts eject",
        "test": "jest",
        "build": "react-scripts build",
        "preview": "npm-run-all --parallel sync start",
        "sync": "browser-sync start -p 'local url' --files '**/*.php' 'build/*.js' 'build/*.css'",
        "dev": "env-cmd -f src/environments/.env.dev react-scripts start",
        "qa": "env-cmd -f src/environments/.env.qa react-scripts start",
        "prod": "env-cmd -f src/environments/.env.prod react-scripts start",
        "build:dev": "env-cmd -f src/environments/.env.dev react-scripts build",
        "build:qa": "env-cmd -f src/environments/.env.qa react-scripts build",
        "build:prod": "env-cmd -f src/environments/.env.prod react-scripts build",
        "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
        "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "eslint": "^8.33.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.4.1",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2"
    }
}
